<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mtt.ls.mapper.DeptMapper">
    <select id="selectDeptById" resultType="mtt.ls.model.Dept">
       select dept_id,dept_name,dept_leader from tbl_dept where dept_id=1
    </select>
</mapper>